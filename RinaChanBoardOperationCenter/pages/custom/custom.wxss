.btn {
    display: inline-block;
    height: 75px;
    background-color: #fff;
}

.btn image {
    width: 100%;
    height: 100%;
    object-fit: cover;
}

.btn2 {
    background-color: #fff;
    border-radius: 10px;
    margin: 0px;
    border: 0px #aaa solid;
    height: 30px;
    padding: 5px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    font-weight: normal;
}