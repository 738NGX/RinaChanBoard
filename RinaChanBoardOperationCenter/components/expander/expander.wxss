.expander{
    margin: 10px;
}

.expander .header{
    padding: 15px;
    background-color: #fff;
    border-bottom: 2px solid #aaa;
    border-right: 2px solid #aaa;
    border-radius: 10px;
}

.expander .content{
    padding: 15px;
    background-color: #fff;
    border-right: 2px solid #aaa;
    border-bottom: 2px solid #aaa;
    border-radius: 0px 0px 10px 10px;
}