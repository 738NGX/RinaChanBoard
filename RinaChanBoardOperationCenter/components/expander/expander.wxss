.expander{
    margin: 10px;
}

.expander .header{
    padding: 10px;
    background-color: #fbfbfb;
    border-bottom: 1px solid #ddd;
    border-right: 1px solid #ddd;
}

.expander .content{
    padding: 10px;
    background-color: #fff;
    border-left: 1px solid #ddd;
    border-right: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
}